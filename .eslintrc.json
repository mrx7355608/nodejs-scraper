{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "quotes": ["error", "double"],
        "linebreak-style": "off",
        "indent": "off",
        "space-before-function-paren": "off",
        "require-jsdoc":"off",
        "no-param-reassign": "off",
        "comma-dangle":"off"
    }
}
